<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="index">
	
	<select id="selectOneRecruitment" resultType="recruitment">
		select * from recruitment where recruitment_no = 2
	</select>
	<select id="selectOneCompany" resultType="company" parameterType="string">
		select * from company where comp_id = #{compId}
	</select>
</mapper>
